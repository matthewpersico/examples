#!/bin/bash

SUBSHELL_TEST_SET=1
export SUBSHELL_TEST_EXPORT=2

alias SUBSHELL_TEST_ALIAS='echo This is SUBSHELL_TEST_ALIAS'

alias SUBSHELL_TEST_ALIAS_EXPORT='echo This is SUBSHELL_TEST_ALIAS_EXPORT'
export SUBSHELL_TEST_ALIAS_EXPORT

SUBSHELL_TEST_FUNC () {
    echo "This is SUBSHELL_TEST_FUNC"
}

SUBSHELL_TEST_FUNC_EXPORT () {
    echo "This is SUBSHELL_TEST_FUNC_EXPORT"
}
safe_func_export SUBSHELL_TEST_FUNC_EXPORT
